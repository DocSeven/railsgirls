
set xrange [0:4]
set yrange [0:20]

set term postscript landscape monochrome "Helvetica" 24
set output "aymerrorbars.ps"

set xlabel "something"
set ylabel "anything"

set key top left

set style data lines
set title "foo bar"

plot 'asymerrorbars.txt' using 1:2:($2+$3):($2-$4) notitle w yerrorbars,\
     'asymerrorbars.txt' using 1:2 title "test" w lines
